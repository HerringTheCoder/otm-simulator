{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "StatusRatio": {
    "Driving": 0.7,
    "Delayed": 0.1,
    "Standing by": 0.2
  },
  "OtmApiConnection": {
    "BasePath": "http://host.docker.internal:8000",
    "Routes": {
      "Timetable": "/timetable/fetch"
    }
  },
  "UpdateInterval": 1
}
